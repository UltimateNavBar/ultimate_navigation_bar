name: Flutter tests

on:
  pull_request:
    branches:
      - fakeDev

jobs:
  flutter_test:
    name: Run flutter tests
    runs-on: ubuntu-latest

    steps:
      - uses: subosito/flutter-action@v2
        with:
          channel: 'stable'

      - name: Run tests
        - run: flutter pub get
        - run: flutter analyze
        - run: flutter test
